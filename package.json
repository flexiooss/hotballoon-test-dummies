{
  "name": "@flexio-oss/hotballoon-test-dummies",
  "version": "1.0.0-dev",
  "description": "Hotballoon test device",
  "main": "index.js",
  "repository": "git@github.com:flexiooss/hotballoon-test-dummies.git",
  "author": "Thomas Chatelain (https://github.com/TomAchT)",
  "license": "Apache-2.0",
  "devDependencies": {
    "code-altimeter-js": "https://github.com/flexiooss/code-altimeter-js.git"
  },
  "dependencies": {
    "@flexio-corp/hotballoon-app-parent": "0.0.0-dev"
  },
  "peerDependencies": {
    "@flexio-oss/global-import-registry": ">0.0.0",
    "@flexio-oss/stylist": ">0.0.0",
    "@flexio-oss/hotballoon": ">O.O.O",
    "@flexio-corp/theme-app-flexio": ">O.O.O",
    "@flexio-oss/js-style-theme-interface": ">O.O.O"
  },
  "hotballoon-shed": {
    "build": {
      "builder": "webpack4"
    },
    "test": {
      "tester": "code-altimeter-js",
      "path": "src/test"
    },
    "browserTest": {
      "path": "src/browserTest"
    },
    "generate-sources": {
      "value-objects": {
        "extension": ".spec"
      }
    }
  }
}
